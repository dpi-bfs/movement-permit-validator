{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2022",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "out",
    "rootDir": "src",
    "baseUrl": ".",
    "sourceMap": true,
    "useUnknownInCatchVariables": false,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/embedding/*"
  ]  
}